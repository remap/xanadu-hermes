  {
    "description": "Move an actor to the location and/or rotation of another actor using their object paths.",
    "request": "/remote/object/call",
    "body": {
      "objectPath": "{{hermes_world}}",
      "functionName": "MoveActorToOverlap",
      "parameters": {
        "SourcePath": "{{bookmark}}",
        "TargetPath": "{{actor}}}",
        "bCopyLocation": "{{location}}",
        "bCopyRotation": "{{rotation}}"
      },
      "generateTransaction": true
    }
  }
