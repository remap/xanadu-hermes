{
  "target_environment": "stage",
  "instance": "jb_testing",
  "metadata": {
    "template_file": "metadata_template.jsont",
    "parser": "jinja2",
    "output_dir": "out"
  },
  "ue": {
    "media_watch_dir": "in",
    "output_dir" : "out",
    "collection_matcher" : "^(?:\\d+|melpomene|calliope|thalia|euterpe|terpsicore|erato|kira)$",
    "upload_on_modified": true,
    "require_depth" : 2
  },
  "s3": {
